{
  "name": "taskatron-client",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "rm -rf dist && parcel build src/index.html --no-source-maps",
    "format": "prettier --write \"src/**/*\"",
    "start": "API_URL='http://localhost:3001' parcel --open --port 3002 src/index.html",
    "test": "run-p test:*",
    "test:format": "prettier --list-different \"src/**/*\"",
    "test:lint": "tslint -c tslint.json 'src/**/*.ts*'",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "apollo-boost": "^0.1.18",
    "classnames": "^2.2.5",
    "eri": "^4.29.0",
    "formik": "^1.0.1",
    "graphql": "^14.0.2",
    "io-ts": "^1.3.0",
    "react": "^16.3.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/graphql": "^14.0.3",
    "@types/node": "^10.7.0",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-dom": "^4.2.6",
    "@types/redux-actions": "^2.3.0",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.13.5",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1"
  }
}
